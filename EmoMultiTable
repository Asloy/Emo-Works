import java.util.Scanner;

public class EmoMultiTable {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter an integer to generate its multiplication table: ");
        int num = scanner.nextInt();

        while (num <= 0) {
            System.out.println(" Kanang Positive ra nga namber ha.");
            System.out.print("Pag type ug number to generate a table : ");
            num = scanner.nextInt();
        }
				System.out.println("Multiplication table for " + num + ":");
        for (int i = 1; i <= 10; i++) {
            int product = num * i;
            System.out.println(num + " x " + i + " = " + product);
        }
    }
}