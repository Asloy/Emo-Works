  } 
     Scanner input=new Scanner(System.in);
 int sum =2 ;
 for (int i =0; i <50 ; i++) {
     for (int a = 2; a<i ; a++){
     if (i%a== 0) {
        break;
     }
 else if (i == a+1){
  sum = sum +i;
